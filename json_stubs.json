/*
	Query 1: 
	Top 10 most popular events in month X
*/
{
	"events": [
		{
			"event_id": 12345, // maybe?
			"event_name": "a string",
			"event_type": "string", // `festival`, `concert` etc..
			"popularity": 0.25, // a number between 0-1
			"age_res": 18, // an age 0-100
			"event_url": "www.event.com/this_event",
			"event_date": "0000-00-00", // YYYYMMDD
			"venue_name": "a string", // 
			"venue_country": "france",
			"venue_city": "paris",
			"artist_name": [
				"jimi hendrix",
				"kurt kubain",
			],
			"artist_url": "www.artist.com/this_artist"
		}
		// this is an array, there will be more of these
	]
}


